{
  "name": "stora-sh",
  "version": "0.1.1",
  "description": "The intelligent mobile app deployment tool",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "stora": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mobile",
    "app-store",
    "google-play",
    "deployment",
    "screenshots",
    "automation",
    "cli"
  ],
  "author": "Carlton Aikins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/storasource/stora.git",
    "directory": "apps/cli"
  },
  "dependencies": {
    "@stora-sh/compliance": "workspace:*",
    "@stora-sh/screenshots": "workspace:*",
    "@stora-sh/shared": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
