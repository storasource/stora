# The URL of your deployed Orchestrator API (e.g., https://stora-api.railway.app)
API_URL=wss://api.stora.sh

# Unique ID for this runner machine (e.g., mac-mini-01)
RUNNER_ID=cloud-mac-01

# Shared token for authenticating with the orchestrator
ORCHESTRATOR_TOKEN=your-secret-token-here

# Vercel Blob Token for uploading video artifacts (Get this from Vercel Dashboard)
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxxxxxxxx
